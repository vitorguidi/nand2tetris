/**
 * 4-way demultiplexor:
 * [a, b, c, d] = [in, 0, 0, 0] if sel = 00
 *                [0, in, 0, 0] if sel = 01
 *                [0, 0, in, 0] if sel = 10
 *                [0, 0, 0, in] if sel = 11
 */
CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;

    PARTS:
    //// Replace this comment with your code.
    DMux(in=in,sel=sel[0],a=dmux2way0, b=dmux2way1);
    Not(in=sel[1],out=notsel1);
    And(a=dmux2way0, b=notsel1, out=a);
    And(a=dmux2way1, b=notsel1, out=b);
    And(a=dmux2way0, b=sel[1], out=c);
    And(a=dmux2way1, b=sel[1], out=d);
}